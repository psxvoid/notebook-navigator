{
  "name": "notebook-navigator",
  "version": "1.7.2",
  "description": "Replace the default file explorer with a clean two-pane interface featuring folder tree, tag browsing, file previews, keyboard navigation, drag-and-drop, pinned notes, and customizable display options.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "transform": "tsx codemods/run-transforms.ts",
    "transform:dry": "tsx codemods/run-transforms.ts --dry-run",
    "transform:imports": "tsx codemods/run-transforms.ts organize-imports",
    "transform:classes": "tsx codemods/run-transforms.ts sort-class-members",
    "transform:react": "tsx codemods/run-transforms.ts organize-react-components",
    "transform:react-internals": "tsx codemods/run-transforms.ts organize-react-internals"
  },
  "keywords": [],
  "author": "Johan Sanneblad",
  "license": "GPL-3.0",
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/js": "^9.32.0",
    "@types/jscodeshift": "^17.3.0",
    "@types/node": "^18.19.120",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitest/coverage-v8": "^3.2.4",
    "builtin-modules": "^5.0.0",
    "esbuild": "^0.25.8",
    "eslint": "^9.32.0",
    "eslint-plugin-obsidianmd": "^0.1.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^15.14.0",
    "jscodeshift": "^17.3.0",
    "knip": "^5.62.0",
    "obsidian": "latest",
    "prettier": "^3.6.2",
    "recast": "^0.23.11",
    "tslib": "^2.8.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.39.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@tanstack/react-virtual": "^3.13.12",
    "date-fns": "^4.1.0",
    "emojilib": "^4.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "smartcrop": "^2.0.5"
  }
}
